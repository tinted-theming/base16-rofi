! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #313235, #313235, #262729

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #313235, #afb2b5, #313235, #313235, #e4e5e6
rofi.color-active:  #313235, #73b3e7, #313235, #313235, #73b3e7
rofi.color-urgent:  #313235, #e77171, #313235, #313235, #e77171

! Set the desired separator style
rofi.separator-style: solid
