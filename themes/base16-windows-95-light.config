! base00: #fcfcfc
! base01: #e0e0e0
! base02: #c4c4c4
! base03: #a8a8a8
! base04: #7e7e7e
! base05: #545454
! base06: #2a2a2a
! base07: #000000
! base08: #a80000
! base09: #fcfc54
! base0A: #a85400
! base0B: #00a800
! base0C: #00a8a8
! base0D: #0000a8
! base0E: #a800a8
! base0F: #54fc54

! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #e0e0e0, #e0e0e0, #fcfcfc

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #e0e0e0, #545454, #e0e0e0, #e0e0e0, #000000
rofi.color-active:  #e0e0e0, #0000a8, #e0e0e0, #e0e0e0, #0000a8
rofi.color-urgent:  #e0e0e0, #a80000, #e0e0e0, #e0e0e0, #a80000

! Set the desired separator style
rofi.separator-style: solid
