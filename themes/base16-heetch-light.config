! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #dedae2, #dedae2, #feffff

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #dedae2, #5a496e, #dedae2, #dedae2, #190134
rofi.color-active:  #dedae2, #5ba2b6, #dedae2, #dedae2, #5ba2b6
rofi.color-urgent:  #dedae2, #f80059, #dedae2, #dedae2, #f80059

! Set the desired separator style
rofi.separator-style: solid
