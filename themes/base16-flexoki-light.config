! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #f2f0e5, #f2f0e5, #fffcf0

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #f2f0e5, #403e3c, #f2f0e5, #f2f0e5, #100f0f
rofi.color-active:  #f2f0e5, #205ea6, #f2f0e5, #f2f0e5, #205ea6
rofi.color-urgent:  #f2f0e5, #af3029, #f2f0e5, #f2f0e5, #af3029

! Set the desired separator style
rofi.separator-style: solid
