! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #e3e5e8, #e3e5e8, #fafafa

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #e3e5e8, #474752, #e3e5e8, #e3e5e8, #2e2e38
rofi.color-active:  #e3e5e8, #6587bf, #e3e5e8, #e3e5e8, #6587bf
rofi.color-urgent:  #e3e5e8, #db7070, #e3e5e8, #e3e5e8, #db7070

! Set the desired separator style
rofi.separator-style: solid
