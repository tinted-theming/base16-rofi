! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #181b1f, #181b1f, #0d0f13

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #181b1f, #dedede, #181b1f, #181b1f, #fffdfb
rofi.color-active:  #181b1f, #6eb2fd, #181b1f, #181b1f, #6eb2fd
rofi.color-urgent:  #181b1f, #f58c81, #181b1f, #181b1f, #f58c81

! Set the desired separator style
rofi.separator-style: solid
