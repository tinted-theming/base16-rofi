! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #282828, #282828, #1d1d1d

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #282828, #c4cbcf, #282828, #282828, #fffef9
rofi.color-active:  #282828, #81a2a2, #282828, #282828, #81a2a2
rofi.color-urgent:  #282828, #ed5a56, #282828, #282828, #ed5a56

! Set the desired separator style
rofi.separator-style: solid
