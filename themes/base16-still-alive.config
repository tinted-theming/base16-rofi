! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #d6d6d6, #d6d6d6, #f0f0f0

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #d6d6d6, #4c383b, #d6d6d6, #d6d6d6, #140c0d
rofi.color-active:  #d6d6d6, #365eff, #d6d6d6, #d6d6d6, #365eff
rofi.color-urgent:  #d6d6d6, #d80000, #d6d6d6, #d6d6d6, #d80000

! Set the desired separator style
rofi.separator-style: solid
