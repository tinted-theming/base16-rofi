! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #242936, #242936, #1f2430

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #242936, #cccac2, #242936, #242936, #f3f4f5
rofi.color-active:  #242936, #73d0ff, #242936, #242936, #73d0ff
rofi.color-urgent:  #242936, #f28779, #242936, #242936, #f28779

! Set the desired separator style
rofi.separator-style: solid
