! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #edeff1, #edeff1, #f8f9fa

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #edeff1, #5c6166, #edeff1, #edeff1, #404447
rofi.color-active:  #edeff1, #399ee6, #edeff1, #edeff1, #399ee6
rofi.color-urgent:  #edeff1, #f07171, #edeff1, #edeff1, #f07171

! Set the desired separator style
rofi.separator-style: solid
