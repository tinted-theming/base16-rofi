! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #e5e5e5, #e5e5e5, #f2f2f2

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #e5e5e5, #767676, #e5e5e5, #e5e5e5, #0c0c0c
rofi.color-active:  #e5e5e5, #0037da, #e5e5e5, #e5e5e5, #0037da
rofi.color-urgent:  #e5e5e5, #c50f1f, #e5e5e5, #e5e5e5, #c50f1f

! Set the desired separator style
rofi.separator-style: solid
