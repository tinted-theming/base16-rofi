! Template author: Tinted Theming (https://github.com/tinted-theming)

! base00: #000000
! base01: #2a2a2a
! base02: #555555
! base03: #808080
! base04: #a1a1a1
! base05: #c0c0c0
! base06: #e0e0e0
! base07: #ffffff
! base08: #ff0000
! base09: #808000
! base0A: #ffff00
! base0B: #00ff00
! base0C: #00ffff
! base0D: #0000ff
! base0E: #ff00ff
! base0F: #008000

! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2a2a2a, #2a2a2a, #000000

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2a2a2a, #c0c0c0, #2a2a2a, #2a2a2a, #ffffff
rofi.color-active:  #2a2a2a, #0000ff, #2a2a2a, #2a2a2a, #0000ff
rofi.color-urgent:  #2a2a2a, #ff0000, #2a2a2a, #2a2a2a, #ff0000

! Set the desired separator style
rofi.separator-style: solid
