! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #3c3836, #3c3836, #282828

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #3c3836, #ebdbb2, #3c3836, #3c3836, #f9f5d7
rofi.color-active:  #3c3836, #458588, #3c3836, #3c3836, #458588
rofi.color-urgent:  #3c3836, #cc241d, #3c3836, #3c3836, #cc241d

! Set the desired separator style
rofi.separator-style: solid
