! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #363636, #363636, #1c1c1c

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #363636, #9e9e9e, #363636, #363636, #d0d0d0
rofi.color-active:  #363636, #5fafd7, #363636, #363636, #5fafd7
rofi.color-urgent:  #363636, #ff5faf, #363636, #363636, #ff5faf

! Set the desired separator style
rofi.separator-style: solid
