! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1e1812, #1e1812, #120f09

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1e1812, #c0a179, #1e1812, #1e1812, #292016
rofi.color-active:  #1e1812, #d6b891, #1e1812, #1e1812, #d6b891
rofi.color-urgent:  #1e1812, #887254, #1e1812, #1e1812, #887254

! Set the desired separator style
rofi.separator-style: solid
