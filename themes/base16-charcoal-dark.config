! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #231b0e, #231b0e, #0f0b05

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #231b0e, #c3a983, #231b0e, #231b0e, #231b0e
rofi.color-active:  #231b0e, #c3a983, #231b0e, #231b0e, #c3a983
rofi.color-urgent:  #231b0e, #a88c62, #231b0e, #231b0e, #a88c62

! Set the desired separator style
rofi.separator-style: solid
