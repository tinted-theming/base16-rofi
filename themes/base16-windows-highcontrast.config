! Template author: Tinted Theming (https://github.com/tinted-theming)

! base00: #000000
! base01: #1c1c1c
! base02: #383838
! base03: #545454
! base04: #a2a2a2
! base05: #c0c0c0
! base06: #dedede
! base07: #fcfcfc
! base08: #fc5454
! base09: #808000
! base0A: #fcfc54
! base0B: #54fc54
! base0C: #54fcfc
! base0D: #5454fc
! base0E: #fc54fc
! base0F: #008000

! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1c1c1c, #1c1c1c, #000000

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1c1c1c, #c0c0c0, #1c1c1c, #1c1c1c, #fcfcfc
rofi.color-active:  #1c1c1c, #5454fc, #1c1c1c, #1c1c1c, #5454fc
rofi.color-urgent:  #1c1c1c, #fc5454, #1c1c1c, #1c1c1c, #fc5454

! Set the desired separator style
rofi.separator-style: solid
