! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #30363d, #30363d, #161b22

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #30363d, #c9d1d9, #30363d, #30363d, #ffffff
rofi.color-active:  #30363d, #388bfd, #30363d, #30363d, #388bfd
rofi.color-urgent:  #30363d, #f85149, #30363d, #30363d, #f85149

! Set the desired separator style
rofi.separator-style: solid
