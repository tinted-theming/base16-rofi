! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2c2737, #2c2737, #211e2a

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2c2737, #e4dee9, #2c2737, #2c2737, #ffffff
rofi.color-active:  #2c2737, #95a6f4, #2c2737, #2c2737, #95a6f4
rofi.color-urgent:  #2c2737, #e965a5, #2c2737, #2c2737, #e965a5

! Set the desired separator style
rofi.separator-style: solid
