! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #131721, #131721, #0b0e14

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #131721, #e6e1cf, #131721, #131721, #f2f0e7
rofi.color-active:  #131721, #59c2ff, #131721, #131721, #59c2ff
rofi.color-urgent:  #131721, #f07178, #131721, #131721, #f07178

! Set the desired separator style
rofi.separator-style: solid
