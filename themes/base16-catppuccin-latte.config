! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #ccd0da, #ccd0da, #eff1f5

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #ccd0da, #4c4f69, #ccd0da, #ccd0da, #7287fd
rofi.color-active:  #ccd0da, #1e66f5, #ccd0da, #ccd0da, #1e66f5
rofi.color-urgent:  #ccd0da, #d20f39, #ccd0da, #ccd0da, #d20f39

! Set the desired separator style
rofi.separator-style: solid
