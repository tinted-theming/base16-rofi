! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #262626, #262626, #161616

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #262626, #f2f4f8, #262626, #262626, #08bdba
rofi.color-active:  #262626, #33b1ff, #262626, #262626, #33b1ff
rofi.color-urgent:  #262626, #ee5396, #262626, #262626, #ee5396

! Set the desired separator style
rofi.separator-style: solid
