! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #313244, #313244, #1e1e2e

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #313244, #cdd6f4, #313244, #313244, #b4befe
rofi.color-active:  #313244, #89b4fa, #313244, #313244, #89b4fa
rofi.color-urgent:  #313244, #f38ba8, #313244, #313244, #f38ba8

! Set the desired separator style
rofi.separator-style: solid
