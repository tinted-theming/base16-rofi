! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #24272b, #24272b, #181b1f

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #24272b, #cecece, #24272b, #24272b, #fffdfb
rofi.color-active:  #24272b, #61a3e6, #24272b, #24272b, #61a3e6
rofi.color-urgent:  #24272b, #df7f78, #24272b, #24272b, #df7f78

! Set the desired separator style
rofi.separator-style: solid
