! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #bcad8c, #bcad8c, #cabda0

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #bcad8c, #382e1b, #bcad8c, #bcad8c, #bcad8c
rofi.color-active:  #bcad8c, #251e0f, #bcad8c, #bcad8c, #251e0f
rofi.color-urgent:  #bcad8c, #382e1b, #bcad8c, #bcad8c, #382e1b

! Set the desired separator style
rofi.separator-style: solid
