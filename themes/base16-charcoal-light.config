! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #c0a179, #c0a179, #d6b891

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #c0a179, #35291d, #c0a179, #c0a179, #d6b891
rofi.color-active:  #c0a179, #120f09, #c0a179, #c0a179, #120f09
rofi.color-urgent:  #c0a179, #413325, #c0a179, #c0a179, #413325

! Set the desired separator style
rofi.separator-style: solid
