! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #3a464c, #3a464c, #333c43

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #3a464c, #d3c6aa, #3a464c, #3a464c, #f3ead3
rofi.color-active:  #3a464c, #7fbbb3, #3a464c, #3a464c, #7fbbb3
rofi.color-urgent:  #3a464c, #e67e80, #3a464c, #3a464c, #e67e80

! Set the desired separator style
rofi.separator-style: solid
